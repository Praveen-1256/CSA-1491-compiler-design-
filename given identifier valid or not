#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char identifier[100];
    int i, len;
    
    printf("Enter an identifier: ");
    scanf("%s", identifier);
    
    len = strlen(identifier);
    
    if (!isalpha(identifier[0]) && identifier[0] != '_') {
        printf("Invalid identifier\n");
        return 0;
    }
    
    for (i = 1; i < len; i++) {
        if (!isalnum(identifier[i]) && identifier[i] != '_') {
            printf("Invalid identifier\n");
            return 0;
        }
    }
    
    printf("Valid identifier\n");
    
    return 0;
}
